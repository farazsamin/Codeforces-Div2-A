/*
Link      :  https://codeforces.com/problemset/problem/1304/A


Approach  :  


 
*/
#include<bits/stdc++.h>
using namespace std;


int main(){
    long long t,x,y,a,b;
    cin>>t;
    while(t--){
    	cin>>x>>y>>a>>b;
    	if((y-x)%(a+b)==0) cout<<(y-x)/(a+b)<<endl;
    	else cout<<"-1"<<endl;
    }

}
