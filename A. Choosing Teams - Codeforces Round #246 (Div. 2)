/*
Problem Link 	:   https://codeforces.com/problemset/problem/432/A


Approach 		 :   count number of students who can participate in ACM, divide it by 3
*/

#include<bits/stdc++.h>
using namespace std;
#define ll long long int 
 
 
int main() {
   int n,k;
   cin>>n>>k;
   int arr[n];
   int cnt=0;
   for(int i=0;i<n;i++) {
     cin>>arr[i];
     if(5-arr[i]>=k){
     	cnt++;
	 }
   }
   cout<<cnt/3<<endl;
  
}
