Problem Link : https://codeforces.com/contest/1405/problem/A


#include<bits/stdc++.h>
using namespace std;
#define ll long long
 
int main()
{
    ll t,n,m,a,b,x,y;
    cin>>t;
    while(t--){
       cin>>n;
       int arr[n+5];
       for(int i=0;i<n;i++)
        cin>>arr[i];
      for(int i=n-1;i>=0;i--){
      	cout<<arr[i]<<" ";
      	
	  }
	  cout<<endl;
      
    }
 
 }
