/*
Problem Link 	: https://codeforces.com/problemset/problem/1337/A


Approach 		:   According to triangle rule,ans will be b,c,c . as b+c > c.
*/

#include<bits/stdc++.h>
using namespace std;


int main(){
    int t;
    cin>>t;
    while(t--){
    	int a,b,c,d;
    	cin>>a>>b>>c>>d;
    	cout<<b<<" "<<c<<" "<<c<<endl;
	}
}
