/*
Problem Link 	: https://codeforces.com/problemset/problem/1369/A


Approach 		:  If n is divisble by 4, then ans is Yes.
*/

#include<bits/stdc++.h>
using namespace std;

 
int main() {
  int t;
  cin>>t;
  while(t--){
        int n;
        cin>>n;
        if(n%4==0) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }
}
