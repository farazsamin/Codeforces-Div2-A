/*
Problem Link 	: https://codeforces.com/problemset/problem/703/A


Approach 		  : Count the round wins by Mishka and Chris.Then print who wins or the match is draw.
*/

#include<bits/stdc++.h>
using namespace std;
#define ll long long int 
 
 
int main() {
    int n,a,b;
    cin>>n;
    int m=0,c=0;
    while(n--){
    	cin>>a>>b;
    	if(a>b) m++;
    	else if(a<b) c++;
	}
	if(m>c) cout<<"Mishka"<<endl;
	else if(m<c) cout<<"Chris"<<endl;
	else cout<<"Friendship is magic!^^";
	
    
  
}
