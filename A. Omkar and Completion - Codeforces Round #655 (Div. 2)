/*
Problem Link 	:  https://codeforces.com/problemset/problem/1372/A


Approach 		  :  If all elements are 1, then answer will be found. 
*/

#include<bits/stdc++.h>
using namespace std;


int main(){
    int t;
    cin>>t;
    while(t--){
    	int n;
    	cin>>n;
    	while(n--) cout<<"1"<<" ";
    	cout<<endl;
	}
}
