/*
Link      :  https://codeforces.com/problemset/problem/1388/A


Approach  :  We can use nearly primes 6,10,14 . If n>30 then ans is yes else ans is no. If n=44,40,36, then ans is  6,10,15,n-31 to show all numbers are distinct.


 
*/
#include<bits/stdc++.h>
using namespace std;


int main(){
    int t,n;
    cin>>t;
    while(t--){
    	cin>>n;
    	if(n>30){
    		cout<<"YES"<<endl;
    		if(n==44 || n==40 ||n==36){
    			cout<<6<<" "<<10<<" "<<15<<" "<<n-31<<endl;
			}else
    		cout<<6<<" "<<10<<" "<<14<<" "<<n-30<<endl;
		}else cout<<"NO"<<endl;
	}


}
