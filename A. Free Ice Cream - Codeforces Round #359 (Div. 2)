/*
Link      :  https://codeforces.com/problemset/problem/686/A


Approach  :


 
*/
#include<bits/stdc++.h>
using namespace std;


int main(){
    long long n,x,d,ans=0,ans2=0;
    cin>>n>>x;
    ans=x;
    for(int i=0;i<n;i++){
    	char ch;
    	cin>>ch>>d;
    	if(ch=='+'){
    		ans+=d;
		}
		else if(ans>=d)
		    ans-=d;
		else 
		    ans2++;
    	
	}
    cout<<ans<<" "<<ans2<<endl;
}
